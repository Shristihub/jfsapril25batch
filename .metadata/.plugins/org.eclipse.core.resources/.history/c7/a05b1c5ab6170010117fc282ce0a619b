package com.shristi.arrays;

public class ArrDemo {
	public static void main(String[] args) {
		
		// declare an array
		int[] marks = new int[5];		
//		System.out.println(marks);
		//initialize
		System.out.println(marks[0]);  //0
		System.out.println("Size "+marks.length);
		
//		populate
		marks[0] = 90;
		marks[1] = 80;
		marks[2] = 70;
		marks[3] = 85;
		
		
		

}
